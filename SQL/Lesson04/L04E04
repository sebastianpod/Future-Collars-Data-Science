SELECT ROUND(AVG(age),2) AS 'Srednia wieku' FROM aggregates.batman;

SELECT ABS(-3) AS 'Wartosc bezwzgledna z -3';

SELECT 2*3 AS 'Wartosc mnozenia';

SELECT 6 /2 AS 'Wyniki dzielenia';
